#include<iostream>
#include<vector>
using namespace std;


int minMoves(vector<vector<int>>& arr, int m, int n){
    int ci = m/2, cj = n/2;
    int onei = 0, onej = 0;
    for(int i=0; i<m; i++){
        for(int j=0; j<n; j++){
            if(arr[i][j] == 1){
                onei = i;
                onej = j;
                break;
            }
        }
    }
    return (abs(ci-onei) + abs(cj-onej));
}


int main(){
    int m;
    int n;
    cin>>m>>n;
   vector<vector<int>> arr(m, vector<int>(n));
    for(int i=0; i<m; i++){
        for(int j=0; j<n; j++){
            cin>>arr[i][j];
        }
    }
    for(int i=0; i<m; i++){
        for(int j=0; j<n; j++){
            cout<<arr[i][j]<<" ";
        }
        cout<<endl;
    }
    cout<<minMoves(arr, m, n);
    return 0;
}
