#include <iostream>
#include<algorithm>
using namespace std;


int numberOfCandies(int arr[], int n, int m){
    sort(arr, arr+n);
    int count = 0;
    int money = m;
    for(int i=0; i<n; i++){
        if(arr[i]%5 == 0){
            count++;
        }
        else{
            if(money >= arr[i]){
                money = money - arr[i];
                count++;
            }
        }
    }
    return count;
}
int main() {
    int n;
    int m;
    cin>>n;
    int arr[n];
    for(int i=0; i<n; i++){
        cin>>arr[i];
    }
    cin>>m;
    cout<<numberOfCandies(arr, n, m);
    return 0;
}
