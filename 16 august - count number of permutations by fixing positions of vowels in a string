#include <iostream>
#include<unordered_map>
using namespace std;

int fact(int num){
    if(num == 0 || num == 1) return 1;
    int ans = 1;
    while(num > 0){
        ans = ans*num--;
    }
    return ans;
}
int main() {
    string s;
    cin>>s;
    int countConsonent = 0;
    unordered_map<char, int> mp;
    mp['A']++;
    mp['E']++;
    mp['I']++; 
    mp['O']++;
    mp['U']++;
    mp['a']++;
    mp['e']++;
    mp['i']++;
    mp['o']++;
    mp['u']++;
    for(int i=0; i<s.length(); i++){
        if(mp.find(s[i]) == mp.end()){
            countConsonent++;
        }
    }
    if(countConsonent == 0){
       cout<<0;
       return 0;
    }
    cout<<fact(countConsonent);
    return 0;
}
