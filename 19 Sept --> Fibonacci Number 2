#include<iostream>
using namespace std;


int fibonacciNumber(int n){
    if(n == 0||n==1) return n;
    if(n == 2) return 1;
    int prev3 = 0;
    int prev2 = 1;
    int prev1 = 1;
    for(int i=3; i<=n; i++){
        int curr = prev3 + prev2 + prev1;
        prev3 = prev2;
        prev2 = prev1;
        prev1 = curr;
    }
    return prev1;
}


int main(){
    int n;
    cin>>n;
    cout<<fibonacciNumber(n);
    return 0;
}
