#include<iostream>
#include<algorithm>
#include<vector>
using namespace std;


bool isPossibleTwoSum(vector<int> arr, int target){
    sort(arr.begin(), arr.end());
    int start = 0;
    int end = arr.size()-1;
    int sum = 0;
    while(start < end){
        sum = arr[start] + arr[end];
        if(sum == target){
            return 1;
        }
        else if(sum < target){
            start++;
        }
        else{
            end--;
        }
    }
    return 0;
}


int main(){
    int n;
    cin>>n;
    int target;
    cin>>target;
    vector<int> arr(n);
    for(int i=0; i<n; i++){
        cin>>arr[i];
    }
    cout<<isPossibleTwoSum(arr, target);
    return 0;
}
