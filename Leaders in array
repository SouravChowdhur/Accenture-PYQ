#include<iostream>
#include<vector>
#include<climits>
#include<algorithm>
using namespace std;


vector<int> leadersArray(vector<int> arr){
    vector<int> ans;
    int maxi = INT_MIN;
    int n = arr.size();
    for(int i=n-1; i>=0; i--){
        if(arr[i] >= maxi){
            ans.push_back(arr[i]);
        }
        maxi = max(arr[i], maxi);
    }
    reverse(ans.begin(), ans.end());
    return ans;
}


int main(){
    int n;
    cin>>n;
    vector<int> arr(n);
    for(int i=0; i<n; i++){
        cin>>arr[i];
    }
    vector<int> ans = leadersArray(arr);
    for(int i=0; i<ans.size(); i++){
        cout<<ans[i]<<" ";
    }
    return 0;
}
